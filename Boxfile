web1:
    name: ajaxdemo
    document_root: /web
    php_extensions:
        - zip
    after_build:
        - "rm web/app_dev.php"
        - "mv web/app.php web/index.php"
        - "mkdir app/cache && chmod 777 app/cache"
        - "mkdir app/logs && chmod 777 app/logs"
        - "curl -s https://getcomposer.org/installer | php"
        - "php composer.phar install"